>\r\n</template>\r\n<script lang=\"ts\">\r\nimport Vue from \"vue\"\r\nimport {mapState} from \"vuex\"\r\n\r\nexport default Vue.extend({\r\n  name: 'MissionSlots',\r\n  computed: {\r\n    ...mapState(['activeGameIndex', 'numGames', 'games', 'phase']),\r\n    ...{\r\n      missions () : any[] {\r\n        const missions = []\r\n        const current = this.activeGameIndex\r\n        for (let i = 0; i < this.numGames; i++) {\r\n          missions.push({\r\n            active: i == current,\r\n            won: i < current,\r\n            battling: this.phase === 'battling'\r\n          })\r\n        }\r\n        return missions\r\n      }\r\n    }\r\n  }\r\n})\r\n</script>\r\n"]}]}